{
    "execution_plan": [
        {
            "id": 1,
            "description": "Find users who have at least one post (from app DB).",
            "database": "postgresql+psycopg://app_user:app_pass@localhost:5440/app",
            "query": "SELECT DISTINCT users.id AS s1__id, users.username AS s1__username, users.email AS s1__email FROM users INNER JOIN posts ON users.id = posts.user_id",
            "depends_on": [],
            "output_columns": [
